#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

void number ( int *a , int *b){
    int sum = *a + *b;
    int diff = abs ( *a - *b);
    *a = sum;
    *b = diff;
}
int main(){
    int a,b ;
    cin >> a >> b;
    number ( &a , &b);
    cout << a << endl << b;
}

