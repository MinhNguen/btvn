#include <bits/stdc++.h>

using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int q ;
    cin >> q;
    set<int>s;
    while ( q--){
        int y,x;
        cin >> y >> x;
        if (y==1){
            s.insert(x);
        }
        else if ( y==2 ){
            s.erase(x);
        }
        else {
            if ( s.find(x) != s.end()){
                cout << "Yes" << " " << endl;
            }
            else {
                cout << "No" << endl;
            }
            
        }
        
        
    }
     
    return 0;
}
